{
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--env-file",
		".devcontainer/devcontainer.env"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go",
				"Vue.vscode-typescript-vue-plugin",
				"Vue.volar"
			]
		}
	},
	"forwardPorts": [
		3000
	],
	"postCreateCommand": "bash -i scripts/install-dev-tools.sh",
	"features": {}
}